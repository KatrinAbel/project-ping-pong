<h2>Profile Overview</h2>

<p>Find out who's playing in your team or a team closeby!</p>

<form>
  <select class="overviewFilter" name="overviewFilter">
    <option></option>
    <option class="option1">wework, Potsdamer Platz</option>
    <option class="option2">wework, at Atrium Tower</option>
  </select>
  <button type="submit" class="btn-overview">apply filter</button>
</form>

<br><br>


  {{#each userData}}
  <div class="card">
  <img class="img" src="{{this.imgPath}}"" alt="Profile-picture">
  <div class="card-body">
    <p class="card-text">Name: {{this.username}}</p>
    <p class="card-text">Level: {{this.level}}</p>
  </div>
  </div> 
  {{/each}} 

