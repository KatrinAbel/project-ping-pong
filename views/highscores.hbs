<br>

<h2>High Scores</h2>
<br>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Position</th>
      <th scope="col">Username</th>
      <th scope="col">Level</th>
      <th scope="col">Points</th>
    </tr>
  </thead>
  {{#each userData}}
  <tbody>
    <tr>
      <th scope="row">{{inc @index}}</th>
      <td>{{this.username}}</td>
      <td>{{this.level}}</td>
      <td>{{this.points}}</td>
      {{/each}}
    </tr>
  </tbody>
</table>