<br>

<h2>High Scores</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Username</th>
      <th scope="col">Team</th>
      <th scope="col">Level</th>
      <th scope="col">Points</th>
    </tr>
  </thead>
     {{#each userData}}
  <tbody>
    <tr>
      <th scope="row">{{@index}}</th>
      <td>{{this.username}}</td>
      <td>{{this.team}}</td>
      <td>{{this.level}}</td>
      <td>{{this.points}}</td>
    {{/each}}
    </tr>
  </tbody>
</table>